<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Boot">


	<update id="startBoot">
		<![CDATA[
			UPDATE MSGBOOT SET
 			sb = #{startBootContent}
		]]>
	</update>
	
	<update id="selectCountBoot">
		<![CDATA[
			UPDATE MSGBOOT SET
 			butnum = #{Num}
		]]>
	</update>
	
	<update id="qaBoot0">
		<![CDATA[
			UPDATE MSGBOOT SET
 			q0 = #{q}, a0 = #{a}
		]]>
	</update>
	
	<update id="qaBoot1">
		<![CDATA[
			UPDATE MSGBOOT SET
 			q1 = #{q}, a1 = #{a}
		]]>
	</update>
	
	<update id="qaBoot2">
		<![CDATA[
			UPDATE MSGBOOT SET
 			q2 = #{q}, a2 = #{a}
		]]>
	</update>
	
	<update id="qaBoot3">
		<![CDATA[
			UPDATE MSGBOOT SET
 			q3 = #{q}, a3 = #{a}
		]]>
	</update>
	
	<select id="BootContent" resultType="boot">
			SELECT * FROM MSGBOOT ;
	</select>
	
</mapper>

